{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "description": "tic tac toe",
  "scripts": {
    "start": "npm-run-all --parallel lint:watch test:watch start:watch",
    "start:watch": "webpack-dev-server",
    "clean": "node node_modules/rimraf/bin.js dist/*",
    "build:dev": "yarn run clean && webpack --config ./webpack.config.dev.js",
    "build:prod": "yarn run clean && webpack --config ./webpack.config.prod.js",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools --ext js --ext jsx",
    "lint:watch": "yarn run lint -- --watch",
    "esl": "eslint ./src --ext jsx --ext js",
    "test": "jest",
    "test:watch": "yarn run test -- --watch"
  },
  "author": "nathan m weller",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "3.3.7",
    "lodash": "4.17.4",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-link": "2.0.2",
    "react-router-redux": "4.0.8",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-jest": "21.0.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "compression": "1.7.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "enzyme": "2.9.1",
    "eslint": "4.6.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.3.0",
    "eslint-watch": "3.1.2",
    "eventsource-polyfill": "0.9.6",
    "expect": "21.0.0",
    "express": "4.15.4",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "jest": "21.0.1",
    "jsdom": "11.2.0",
    "mocha": "3.5.0",
    "nock": "9.0.14",
    "npm-run-all": "4.0.2",
    "open": "0.0.5",
    "react-addons-test-utils": "15.6.0",
    "react-test-renderer": "15.6.1",
    "redux-immutable-state-invariant": "2.0.0",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.6",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.7.1",
    "webpack-hot-middleware": "2.19.1"
  },
  "jest": {
    "verbose": true
  },
  "repository": {
    "type": "git"
  }
}
