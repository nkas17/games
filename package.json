{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "description": "tic tac toe",
  "scripts": {
    "start": "npm-run-all --parallel lint:watch test:watch start:watch",
    "start:watch": "webpack-dev-server",
    "clean": "node node_modules/rimraf/bin.js dist/*",
    "build": "npm run clean && webpack --config ./webpack.config.prod.js",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src --ext js --ext jsx",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "author": "nathan m weller",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "3.4.1",
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-hot-loader": "4.12.15",
    "react-router-dom": "5.1.2",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "babel-loader": "8.0.6",
    "cheerio": "0.22.0",
    "copy-webpack-plugin": "5.0.4",
    "cross-env": "6.0.3",
    "css-loader": "3.2.0",
    "enzyme": "2.9.1",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-watch": "6.0.1",
    "file-loader": "4.2.0",
    "jest": "24.9.0",
    "npm-run-all": "4.0.2",
    "prettier": "1.18.2",
    "react-addons-test-utils": "15.6.0",
    "react-test-renderer": "15.6.1",
    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "url-loader": "2.2.0",
    "webpack": "4.41.1",
    "webpack-dev-middleware": "3.7.2",
    "webpack-dev-server": "3.8.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-cli": "3.3.9"
  },
  "jest": {
    "verbose": true
  },
  "repository": {
    "type": "git"
  }
}
